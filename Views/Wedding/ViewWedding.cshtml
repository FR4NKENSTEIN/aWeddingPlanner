
@model Wedding

<partial name="NavPartial"></partial>
<h1 class="text-center my-3">@Model.WedderOne and @Model.WedderTwo's Wedding</h1>
<div class="row my-3">
    <div class="col">
        <h3>@Model.Ceremony</h3>
    </div>
    <div class="col">
        <h4>@Model.Address</h4>
    </div>
</div>
<div class="row my-2">
    <div class="col">
        <h2>Guests List</h2>
        <hr>
        @foreach (Relation r in @Model.Guests)
        {
            <h5>@r.Guest.FirstName @r.Guest.LastName</h5>
        }
    </div>
    <div class="col"></div>
</div>
